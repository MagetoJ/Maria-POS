{
  "name": "maria-havens-pos-backend",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec ts-node src/index.ts",
    "setup-db": "node setup-postgres.js",
    "setup-production-db": "node setup-production-db.js",
    "migrate:dev": "cross-env NODE_ENV=development npx knex migrate:latest",
    "migrate:prod": "cross-env NODE_ENV=production npx knex migrate:latest",
    "seed:dev": "cross-env NODE_ENV=development npx knex seed:run",
    "seed:prod": "cross-env NODE_ENV=production npx knex seed:run",
    "fix-images:dev": "cross-env NODE_ENV=development node fix-image-columns.js",
    "fix-images:prod": "cross-env NODE_ENV=production node fix-image-columns.js",
    "setup-render": "cross-env NODE_ENV=production node setup-render-database.js",
    "db:reset:dev": "cross-env NODE_ENV=development npx knex migrate:rollback --all && npx knex migrate:latest && npx knex seed:run"
  },
  "dependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/nodemailer": "^7.0.2",
    "@types/pg": "^8.10.7",
    "@types/ws": "^8.5.8",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.9",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7",
    "typescript": "^5.9.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1"
  }
}
